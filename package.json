  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:client": "rimraf ./dist --prod && ng build",
    "build:universal": "ng build --prod && ng run nest-angular:server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server": "sudo chmod a+rwX ./bin && rimraf ./bin --prod && webpack --progress --colors",
    "start1": "concurrent \"  npm run build:client \" \" ts-node --experimental-worker -r  ./src/server/main.ts  \" ",
    "build": "npm run build:universal && NODE_ENV=production  npm run build:server",
    "watch:server": "rimraf ./bin && webpack --w --colors",
    "watch:client": "ng serve --proxy-config proxy.conf.json",
    "watch": "concurrent \" npm run watch:server \" \" npm run watch:client \" ",
    "prod": "concurrent \" npm run build:universal \" \"  npm run build:server \" "
  },
